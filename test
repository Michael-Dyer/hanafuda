let arr = ["apple", "mango",
          "apple", "orange", "mango", "mango"];
  
function removeDuplicates(arr) {
    let unique = arr.reduce(function (acc, curr) {
        if (!acc.includes(curr))
            acc.push(curr);
        return acc;
    }, []);
    return unique;
}

arr = removeDuplicates(arr)
console.log(arr);

this.chaff  = removeDuplicates()
this.animal = removeDuplicates()
this.ribbon = removeDuplicates()
this.lights = removeDuplicates()


this.BDB = removeDuplicates()
this.RPR = removeDuplicates()
this.BPR = removeDuplicates()
this.CBV = removeDuplicates()
this.MV = removeDuplicates()
this.LWR =  removeDuplicates()